{
	"name": "dataflow8",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DelimitedText36",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "Json9",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "filter1"
				}
			],
			"scriptLines": [
				"parameters{",
				"     param1 as string",
				"}",
				"source(output(",
				"          ID as integer,",
				"          Firstname as string,",
				"          LastName as string,",
				"          City as string,",
				"          StartDate as date",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 filter(City == $param1) ~> filter1",
				"filter1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          Name as string,",
				"          Gender as string,",
				"          DOB as string,",
				"          Pets as string[],",
				"          Address as ({House Number} as string, City as string, State as string),",
				"          Phone as (Work as integer, Mobile as integer),",
				"          filepath as string,",
				"          filename as string,",
				"          dbname as string,",
				"          tablename as string",
				"     ),",
				"     partitionFileNames:['output2908.json'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> sink1"
			]
		}
	}
}